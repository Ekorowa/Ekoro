@page "/about"
@inject IOrchardHelper OrchardHelper;
@model AboutModel
@{
    ViewData["Title"] = Model.ContentItem.DisplayText;
}

@if (Model.ContentItem != null && Model.ContentItem.Content.LiquidPart != null)
{
    var html = (await OrchardHelper.LiquidToHtmlAsync((string)Model.ContentItem.Content.LiquidPart.Liquid)).GetString();
    @Html.Raw(html)
}

